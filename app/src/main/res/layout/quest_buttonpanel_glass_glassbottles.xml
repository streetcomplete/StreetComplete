<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:parentTag="com.google.android.flexbox.FlexboxLayout"
    tools:flexWrap="wrap"
    tools:justifyContent="space_between">

    <Button
        android:id="@+id/glassButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/BottomSheetButtonBarItem"
        android:text="@string/quest_recycling_type_any_glass" />

    <Button
        android:id="@+id/glassBottlesButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/BottomSheetButtonBarItem"
        android:text="@string/quest_recycling_type_glass_bottles_short" />

</merge>
